									<label for="dance">Dance</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="dataScience" name="dataScience" <% if(user.subjects.includes("dataScience")) { %> checked <% } %>>
									<label for="dataScience">Data Science</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="earthAndEnvironmentalSciences" name="earthAndEnvironmentalSciences" <% if(user.subjects.includes("earthAndEnvironmentalSciences")) { %> checked <% } %>>
									<label for="earthAndEnvironmentalSciences">Earth and Environmental Sciences</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="economics" name="economics" <% if(user.subjects.includes("economics")) { %> checked <% } %>>
									<label for="economics">Economics</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="education" name="education" <% if(user.subjects.includes("education")) { %> checked <% } %>>
									<label for="education">Education</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="english" name="english" <% if(user.subjects.includes("english")) { %> checked <% } %>>
									<label for="english">English</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="geology" name="geology" <% if(user.subjects.includes("geology")) { %> checked <% } %>>
									<label for="geology">Geology</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="history" name="history" <% if(user.subjects.includes("history")) { %> checked <% } %>>
									<label for="history">History</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="law" name="law" <% if(user.subjects.includes("law")) { %> checked <% } %>>
