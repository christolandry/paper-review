<!-- Head -->
<%- include('partials/head') -%>

<!-- Header -->
	<%- include('partials/header') -%>

<!-- Menu -->
	<%- include('partials/nav') -%>

<!-- Main -->
	<article id="main">
		<div class="wrapper">
			<div class="inner">
				<!-- Elements -->
					<h2 id="elements">Submit a Paper</h2>
						<form action="/papers/submit" enctype="multipart/form-data" method="POST">
							<div class="row gtr-uniform">
								<div class="col-6 col-12-xsmall">
									<label for="title">Paper Title</label>
									<input type="text" name="title" id="title" value="" placeholder="Paper Title" />
								</div>
								<div class="col-6 col-12-xsmall">
								  <label for="discipline">Paper Discipline</label>
								  <select name="discipline" id="discipline">
									<option value="">- Discipline -</option>
									<option value="Africana Studies">Africana Studies</option>
									<option value="Anthropology">Anthropology</option>
									<option value="Art History">Art History</option>
									<option value="Biology">Biology</option>
									<option value="Business">Business</option>
									<option value="Chemistry">Chemistry</option>
									<option value="Classics">Classics</option>
									<option value="Communications">Communications</option>
									<option value="Comparative Literature">Comparative Literature</option>
									<option value="Computer Science">Computer Science</option>
									<option value="Dance">Dance</option>
									<option value="Data Science">Data Science</option>
									<option value="Earth and Environmental Sciences">Earth and Environmental Sciences</option>
									<option value="Economics">Economics</option>
									<option value="Education">Education</option>
									<option value="English">English</option>
									<option value="Geology">Geology</option>
									<option value="History">History</option>
									<option value="Law">Law</option>
									<option value="Language and Culture">Language and Culture</option>
									<option value="Linguistics">Linguistics</option>
									<option value="Mathematics">Mathematics</option>
									<option value="Music">Music</option>
									<option value="Philosophy">Philosophy</option>
									<option value="Physics">Physics</option>
									<option value="Political Science">Political Science</option>
									<option value="Psychology">Psychology</option>
									<option value="Public Policy">Public Policy</option>
									<option value="Religious Studies">Religious Studies</option>
									<option value="Sociology">Sociology</option>
									<option value="Theater and Performance">Theater and Performance</option>
								  </select>
								</div>
								<!-- Break -->
								<div class="col-12">
									<h5>Sub-Disciplines</h5>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="africanaStudies" name="africanaStudies" <% if(user.subjects.includes("africanaStudies")) { %> checked <% } %>>
									<label for="africanaStudies">Africana Studies</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="anthropology" name="anthropology" <% if(user.subjects.includes("anthropology")) { %> checked <% } %>>
									<label for="anthropology">Anthropology</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="artHistory" name="artHistory" <% if(user.subjects.includes("artHistory")) { %> checked <% } %>>
									<label for="artHistory">Art History</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="biology" name="biology" <% if(user.subjects.includes("biology")) { %> checked <% } %>>
									<label for="biology">Biology</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="business" name="business" <% if(user.subjects.includes("business")) { %> checked <% } %>>
									<label for="business">Business</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="chemistry" name="chemistry" <% if(user.subjects.includes("chemistry")) { %> checked <% } %>>
									<label for="chemistry">Chemistry</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="classics" name="classics" <% if(user.subjects.includes("classics")) { %> checked <% } %>>
									<label for="classics">Classics</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="communications" name="communications" <% if(user.subjects.includes("communications")) { %> checked <% } %>>
									<label for="communications">Communications</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="comparativeLiterature" name="comparativeLiterature" <% if(user.subjects.includes("comparativeLiterature")) { %> checked <% } %>>
									<label for="comparativeLiterature">Comparative Literature</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="computerScience" name="computerScience" <% if(user.subjects.includes("computerScience")) { %> checked <% } %>>
									<label for="computerScience">Computer Science</label>
								</div>
								<div class="col-3 col-12-small">
