									<label for="languageAndCulture">Language and Culture</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="linguistics" name="linguistics" <% if(user.subjects.includes("linguistics")) { %> checked <% } %>>
									<label for="linguistics">Linguistics</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="mathematics" name="mathematics" <% if(user.subjects.includes("mathematics")) { %> checked <% } %>>
									<label for="mathematics">Mathematics</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="music" name="music" <% if(user.subjects.includes("music")) { %> checked <% } %>>
									<label for="music">Music</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="philosophy" name="philosophy" <% if(user.subjects.includes("philosophy")) { %> checked <% } %>>
									<label for="philosophy">Philosophy</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="physics" name="physics" <% if(user.subjects.includes("physics")) { %> checked <% } %>>
									<label for="physics">Physics</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="politicalScience" name="politicalScience" <% if(user.subjects.includes("politicalScience")) { %> checked <% } %>>
									<label for="politicalScience">Political Science</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="psychology" name="psychology" <% if(user.subjects.includes("psychology")) { %> checked <% } %>>
									<label for="psychology">Psychology</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="publicPolicy" name="publicPolicy" <% if(user.subjects.includes("publicPolicy")) { %> checked <% } %>>
									<label for="publicPolicy">Public Policy</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="religiousStudies" name="religiousStudies" <% if(user.subjects.includes("religiousStudies")) { %> checked <% } %>>
									<label for="religiousStudies">Religious Studies</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="sociology" name="sociology" <% if(user.subjects.includes("sociology")) { %> checked <% } %>>
									<label for="sociology">Sociology</label>
								</div>
								<div class="col-3 col-12-small">
									<input type="checkbox" id="theaterAndPerformance" name="theaterAndPerformance" <% if(user.subjects.includes("theaterAndPerformance")) { %> checked <% } %>>
									<label for="theaterAndPerformance">Theater and Performance</label>
								</div>
								<!-- Break -->
								<div class="col-6 col-12-xsmall">
									<label for="description">Paper Description</label>
									<textarea name="description" id="description" placeholder="Description" rows="3" maxlength="150"></textarea>
								</div>
								<div class="col-6 col-12-xsmall">
									<label for="keywords">Keywords (comma separated)</label>
									<textarea name="keywords" id="keywords" placeholder="Keyword one, keyword two" rows="3" maxlength="50"></textarea>
								</div>
								<!-- Break -->
								<div class="col-6 col-12-xsmall">
									<label for="feedback">Specific Feedback Requested</label>
									<textarea name="feedback" id="feedback" placeholder="Specific feedback requests" rows="3" maxlength="150"></textarea>
								</div>
								<div class="col-6 col-12-xsmall">
									<label for="feedback">Number of Reviews Requested</label>
									<select name="category" id="category">
										<option value="1">1</option>
										<!-- <option value="2">2</option>
										<option value="3">3</option>
										<option value="4">4</option>
										<option value="5">5</option>
										<option value="6">6</option>
										<option value="7">7</option>
										<option value="8">8</option>
										<option value="9">9</option>
										<option value="10">10</option> -->
									</select>
								</div>
								<!-- Break -->
								<div class="col-12">
									<input type="file" id="imageUpload" name="file">
								</div>
								<!-- Break -->
								<div class="col-12">
									<ul class="actions">
										<li><input type="submit" value="Submit Paper" class="primary" /></li>
										<li><input type="reset" value="Reset" /></li>
									</ul>
								</div>
							</div>
						</form>
	</article>

<!-- Footer -->
	<%- include('partials/footer') -%>

<!-- Scripts & Close -->
	<%- include('partials/scripts') -%>
	